{
  "name": "twitch",
  "version": "1.0.0",
  "description": "FreeCodeCamp Twitch exercise",
  "repository": {
    "type": "git",
    "url": "http://github.com/jw120/fcc-twitch.git"
  },
  "main": "index.html",
  "dependencies": {
    "font-awesome": "^4.6.3"
  },
  "devDependencies": {
    "buble": "^0.10.6",
    "tslint": "next",
    "typescript": "next"
  },
  "scripts": {
    "compile": "tsc",
    "compile-watch": "tsc --watch",
    "lint": "tslint -c tslint.json src/twitch.ts",
    "build:js": "buble src/twitch.js --output dist/twitch.js",
    "build:static": "cp src/index.html src/twitch.css dist/",
    "build": "npm run build:js && npm run build:static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "JW",
  "license": "MIT"
}
